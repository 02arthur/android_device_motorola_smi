<?xml version="1.0" encoding="UTF-8"?>
  <manifest>

    <remote  name="cyanogenmod"
           fetch="git://github.com/CyanogenMod/"/>

    <remote  name="hazouph"
           fetch="git://github.com/HazouPH/"/>

    <remote  name="oxavelar"
           fetch="git://github.com/oxavelar/"/>

    <remote  name="twrp"
           fetch="git://github.com/TeamWin/"/>

    <remote  name="android-ia"
           fetch="https://android-review.01.org/"/>

    <project path="device/motorola/smi"
      name="android_device_motorola_smi"
             remote="hazouph"
             revision="jellybean"/>

    <project path="device/motorola/smi-plus"
      name="android_device_motorola_smi-plus"
             remote="hazouph"
             revision="jellybean"/>

    <project path="bootable/recovery"
      name="Team-Win-Recovery-Project"
             remote="twrp"
             revision="twrp2.5"/>
