     <remove-project name="Team-Win-Recovery-Project" />

    <project path="external/pciutils"
      name="platform/external/pciutils"
             remote="android-ia"
             revision="release/android-4.2.2_r1-ia0"/>

    <project path="external/drm"
      name="fdo/chadversary/drm"
             remote="android-ia"
             revision="release/android-4.2.2_r1-ia0"/>

    <project path="external/powertop"
      name="platform/external/powertop"
             remote="android-ia"
             revision="release/android-4.2.2_r1-ia0"/>
     <remove-project name="CyanogenMod/android_external_powertop" />

  </manifest>

